{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "react/prop-types": 0,
        "arrow-parens": [
            "warn",
            "as-needed"
        ],
        "no-use-before-define": "warn",
        "no-mixed-operators": [
            "warn"
        ],
        "curly": [
            "error",
            "multi-or-nest"
        ],
        "nonblock-statement-body-position": [
            "error",
            "below"
        ],
        "no-nested-ternary": [
            "warn"
        ]
    }
}